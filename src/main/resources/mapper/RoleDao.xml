<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdut.imis.esclientdemo.dao.RoleDao">
<insert id="insertRole" parameterType="com.gdut.imis.esclientdemo.entity.Role">
    insert into role values(#{id},#{roleName},#{AroleJob})
</insert>
    <select id="getRoleById" resultType="com.gdut.imis.esclientdemo.entity.Role">
        select * from role where id = #{id}
    </select>
</mapper>
